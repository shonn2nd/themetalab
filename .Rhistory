set.seed(42)
# Parameters
n_individuals <- 100
n_timepoints <- 6
# Create individual and time identifiers
id <- rep(1:n_individuals, each = n_timepoints)
time <- rep(1:n_timepoints, times = n_individuals)
# Individual-level random intercepts
intercepts <- rnorm(n_individuals, mean = 0, sd = 1)
intercept_vec <- intercepts[id]
# Time-varying predictor
x <- rnorm(n_individuals * n_timepoints, mean = 0, sd = 1)
# Outcome
y <- 1.5 * x + 0.3 * time + intercept_vec + rnorm(n_individuals * n_timepoints, 0, 1)
# Create data frame
df_long <- data.frame(id = id, time = time, x = x, y = y)
